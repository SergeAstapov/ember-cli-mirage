{
  "name": "basic-app",
  "version": "0.0.0",
  "private": true,
  "description": "Small description for basic-app goes here",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": "",
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test --test-port=0",
    "test:fastboot": "qunit fastboot-tests/**/*.js"
  },
  "devDependencies": {
    "@ember/test-helpers": "*",
    "@glimmer/component": "*",
    "@glimmer/tracking": "*",
    "babel-eslint": "*",
    "broccoli-asset-rev": "*",
    "ember-auto-import": "*",
    "ember-cli": "*",
    "ember-cli-babel": "*",
    "ember-cli-dependency-checker": "*",
    "ember-cli-fastboot": "*",
    "ember-cli-htmlbars": "*",
    "ember-cli-inject-live-reload": "*",
    "ember-cli-mirage": "*",
    "ember-cli-sri": "*",
    "ember-cli-terser": "*",
    "ember-data": "*",
    "ember-export-application-global": "*",
    "ember-fetch": "*",
    "ember-load-initializers": "*",
    "ember-maybe-import-regenerator-for-testing": "*",
    "ember-qunit": "*",
    "ember-resolver": "*",
    "ember-source": "*",
    "ember-template-lint": "*",
    "eslint": "*",
    "eslint-config-prettier": "*",
    "eslint-plugin-ember": "*",
    "eslint-plugin-node": "*",
    "eslint-plugin-prettier": "*",
    "eslint-plugin-qunit": "*",
    "faker": "*",
    "fastboot": "*",
    "jsdom": "*",
    "loader.js": "*",
    "npm-run-all": "*",
    "prettier": "*",
    "qunit": "*",
    "qunit-dom": "*",
    "webpack": "^5.58.2"
  },
  "engines": {
    "node": "12.* || 14.* || >= 16"
  },
  "ember": {
    "edition": "octane"
  }
}
